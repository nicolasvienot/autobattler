{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vite/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@nico/battle-core/*": [
        "../../packages/battle-core/src/*"
      ],
      "@nico/battle-core": [
        "../../packages/battle-core/src/index.ts"
      ]
    },
    "composite": true
  },
  "include": [
    "src"
  ]
}